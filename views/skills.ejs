<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Skills Page</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
    <h1>Skills List</h1>
    <ol>
        <% skills.forEach(function(skill){ %>
            <li> <a href="http://localhost:3000/skills/<%= `${skill.id}` %>">
                    <%= `${skill.skillName}` %>
                </a> <br> </li>
            <% }) %>
    </ol>
    <a href="/skills/new">Add skill</a>
</body>
</html>